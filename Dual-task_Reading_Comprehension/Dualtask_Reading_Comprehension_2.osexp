define loop corsi_loop
	set source_file ""
	set source table
	set repeat 1
	set order random
	set description "Repeatedly runs another item"
	set cycles 6
	set continuous no
	set break_if_on_first yes
	set break_if never
	setcycle 0 xcoor -448
	setcycle 0 ycoor -320
	setcycle 1 xcoor -448
	setcycle 1 ycoor 320
	setcycle 2 xcoor 448
	setcycle 2 ycoor -320
	setcycle 3 xcoor 448
	setcycle 3 ycoor 320
	setcycle 4 xcoor -192
	setcycle 4 ycoor 0
	setcycle 5 xcoor 224
	setcycle 5 ycoor 0
