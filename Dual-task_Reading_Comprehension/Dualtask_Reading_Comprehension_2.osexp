define inline_javascript define_variables
	set description "Executes JavaScript code"
	___run__
	vars.numtime = 1000
	vars.nums_isi = 500
	
	vars.practice_round = 1
	vars.trial_round = 1
	__end__
	set _prepare ""

define sequence experiment
	set flush_keyboard yes
	set description "Runs a number of items in sequence"
	run define_variables always
	run Greeting always
	run form_multiple_choice_age always
	run form_multiple_choice_L1 always
	run form_multiple_choice_diagnose always
	run Intro_1 always
	run Intro_2 always
	run Intro_3 always
	run practice_loop always
	run start always
	run trials_loop always

