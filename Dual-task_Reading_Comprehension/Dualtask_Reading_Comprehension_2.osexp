define sketchpad Greeting
	set duration keypress
	set description "Displays stimuli"
	draw textline center=1 color=white font_bold=no font_family=mono font_italic=no font_size=18 html=yes show_if=always text="Tak fordi du har valgt at deltage i undersøgelsen. <br /><br />Inden undersøgelsen bedes du besvare følgende spørgsmål. Alle besvarelser er anonyme. <br /><br /><br />Tryk på mellemrum for at fortsætte." x=0 y=0 z_index=0

