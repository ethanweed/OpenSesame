define form_multiple_choice form_multiple_choice_L1
	set timeout infinite
	set spacing 10
	set question "Er dansk dit modersmål?"
	__options__
	Ja
	Nej
	__end__
	set margins "50;50;50;50"
	set form_var response
	set form_title "Form title"
	set description "A simple multiple choice item"
	set button_text Ok
	set allow_multiple no
	set advance_immediately yes
	set _theme gray

define form_multiple_choice form_multiple_choice_age
	set timeout infinite
	set spacing 10
	__question__
	Hvad er din alder?
	__end__
	__options__
	Under 15 år
	15-20 år
	21-30 år
	31-40 år
	Over 40 år
	__end__
	set margins "50;50;50;50"
	set form_var response
	set form_title "Form title"
	set description "A simple multiple choice item"
	set button_text Ok
	set allow_multiple no
	set advance_immediately yes
	set _theme gray

define form_multiple_choice form_multiple_choice_diagnose
	set timeout infinite
	set spacing 10
	set question "Har du en diagnose, som indebærer opmærksomhedsforstyrrelser?"
	__options__
	Ja
	Nej
	Ønsker ikke at svare
	__end__
	set margins "50;50;50;50"
	set form_var response
	set form_title "Form title"
	set description "A simple multiple choice item"
	set button_text Ok
	set allow_multiple no
	set advance_immediately yes
	set _theme gray

