	draw textline center=1 color=white font_bold=no font_family=mono font_italic=no font_size=18 html=yes show_if=always text="Du vil blive præsenteret for en række historier<br /><br />En sætning vil blive vist ad gangen. <br /><br />Brug mellemrumstasten efter hver sætning for at gå videre til den næste. <br /><br /><br /><br />Tryk på mellemrum for at fortsætte" x=0 y=0 z_index=0

