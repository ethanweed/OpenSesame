define sequence response_sequence
	set flush_keyboard yes
	set description "Runs a number of items in sequence"
	run prompt always
	run new_mouse_response always
	run new_inline_javascript always
	run new_feedback "[cursor_x] > -448 & [cursor_x] < -352"
	run new_feedback_1 "[cursor_x] > -352 & [cursor_x] < - 192"
	run new_feedback_2 "[cursor_x] > -192 & [cursor_x] < -32"
	run new_feedback_3 "[cursor_x] > 32 & [cursor_x]< 128"
	run new_feedback_4 "[cursor_x] > 128 & [cursor_x] < 288"
	run new_feedback_5 "[cursor_x] > 288 & [cursor_x] < 448"
	run new_logger always

